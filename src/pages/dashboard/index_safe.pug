                                  H2TEXT.mb-0.info-card__number.grey_900 #{card.outOfTime.number}
                                  a(href="#").px-xs #{card.outOfTime.text}
                                  i.fas.fa-exclamation-triangle.text--status_critical_500
                                div.px-xs.py-xs.warning-border   
