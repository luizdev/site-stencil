block content
  main.bg-blue.dashboard
   .col-12               
      .container-flex.main
        .row
          .col-12
            .row.mb-m.mt-m  
              //- Left content
              .col-9
                div.content-aside-left 
                  label Aquí va el contenido
                  //- Top card
                  .card.px-s.py-s.mb-s
                    .card-header 
                      h1 Hola Francisco
                      p.content-label.mb-m Esto es lo que necesitas saber para hoy
                    .card-content
                      div.display-f 
                        div.w50 
                          div.display-f
                            div.w50.general-card
                              h5 PRIMEROS SERVICIOS 
                              h2 10.145
                              .display-f.positive.flex--align-center
                                i(class="fa fa-long-arrow-alt-up general-card__icon")
                                label.general-card__percentage +2%
                                p.m-n.p-n.general-card__content Videoperitaciones
                            div.w50
                              label Aquí va la gráfica
                        div.w50 
                          div.display-f
                            div.w50.general-card
                              h5 CLONES
                              h2 12.145
                              .display-f.negative.flex--align-center 
                                i(class="fa fa-long-arrow-alt-down general-card__icon")
                                label.general-card__percentage -2%
                                p.m-n.p-n.general-card__content Videoperitaciones
                            div.w50
                              label Aquí va la gráfica
                  //- Info cards
                  .row.info-cards
                    each card in topCards
                      .col-3 
                        .card.info-card
                          .card-content.p-xs
                            div.display-f.flex--align-center 
                              i(class="fa "+card.icon+' info-card__icon mr-xs')
                              h5.m-n.info-card__title.grey #{card.title}
                            h2.mb-0.info-card__number.grey #{card.number}
                            small.info-card__description.grey #{card.description}
                  

                  //- General dashboard cards
                  div.display-f.flex--justify-between.critical-border.dashboard-image
                    div.display-f.flex--column.flex--justify-around.mb-l
                      each card in middleCards 
                        .card.info-card
                          .card-content
                            div.display-f.flex--align-center
                              div
                                h2.m-n.info-card__title.grey #{card.tasks}
                              div
                                a(href="#").px-xs.special-link #{card.text}
                    div.display-f
                      div.display-f.mx-s.flex--column
                        each card in middleCards 
                          div.display-f.flex--column
                            .card.info-card.mt-s.py-15.px-xs.critical-border 
                              .card-content                                  
                                div.display-f.flex--justify-between
                                  div
                                    H2TEXT #{card.outOfTime.number}
                                    a(href="#").px-xs #{card.outOfTime.text}
                                  div  
                                  i.fas.fa-exclamation-triangle.text--status_critical_500
                            .card.info-card.my-s.py-15.px-xs.warning-border
                              .card-content  
                                div.display-f.flex--justify-between
                                  div
                                    H2TEXT #{card.today.number}
                                    a(href="#").px-xs #{card.today.text}
                                  div
                                  i.fas.fa-clock.text--status_warning_500
                            .card.info-card.mb-s.py-15.px-xs.services-border
                              .card-content  
                                div.display-f
                                  H2TEXT.grey #{card.services.number}
                                  a(href="#").px-xs #{card.services.text}                           

                      div.display-f.flex--column
                        each card in middleCards
                          div.display-f.flex--column
                            .card.info-card.py-15.px-xs.mt-s
                              .card-content
                                div.display-f.flex--align-center
                                  div.display-f.flex--align-center.mx-s
                                    H3TEXT #{card.outOfTime.pendContact.number}
                                    small.px-xs  #{card.outOfTime.pendContact.text}
                                  div.display-f.flex--align-center.mx-s
                                    H3TEXT #{card.outOfTime.pendVisita.number}
                                    small.info-card__description.grey.px-xs  #{card.outOfTime.pendVisita.text}
                                  div.display-f.flex--align-center.mx-s
                                    H3TEXT #{card.outOfTime.pendAvance.number}
                                    small.info-card__description.grey.px-xs  #{card.outOfTime.pendAvance.text}
                                  div.display-f.flex--align-center.mx-s
                                    H3TEXT #{card.outOfTime.pendInforme.number}
                                    small.info-card__description.grey.px-xs  #{card.outOfTime.pendInforme.text} 
                            .card.info-card.my-s.py-15.px-xs
                              .card-content
                                div.display-f.flex--align-center
                                  div.display-f.flex--align-center.mx-s
                                    H3TEXT #{card.today.pendContact.number}
                                    small.px-xs  #{card.outOfTime.pendContact.text}
                                  div.display-f.flex--align-center.mx-s
                                    H3TEXT #{card.today.pendVisita.number}
                                    small.info-card__description.grey.px-xs  #{card.today.pendVisita.text}
                                  div.display-f.flex--align-center.mx-s
