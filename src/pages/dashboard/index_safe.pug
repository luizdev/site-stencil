                                  div.display-f.flex--align-center.mx-s
                                    H3TEXT #{card.services.pendContact.number}
                                    small.px-xs  #{card.services.pendContact.text}
                                  div.display-f.flex--align-center.mx-s
                                    H3TEXT #{card.services.pendVisita.number}
